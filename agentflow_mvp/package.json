{
  "name": "agentflow_mvp",
  "version": "1.0.0",
  "description": "AgentFlow 2.0 MVP Skeleton with Web UI (150/100)",
  "main": "src/server.mjs",
  "type": "module",
  "bin": {
    "agentflow": "src/server.mjs"
  },
  "scripts": {
    "start": "node src/server.mjs",
    "test": "vitest run",
    "build": "esbuild src/server.mjs --bundle --platform=node --outfile=dist/server.cjs --format=cjs",
    "build:exe": "npm run build && pkg dist/server.cjs --targets node18-win-x64 --output agentflow-launcher.exe"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "vis-network": "^9.1.2",
    "@google/genai": "^0.15.0"
  },
  "devDependencies": {
    "vitest": "^1.0.1",
    "pkg": "^5.8.1",
    "esbuild": "^0.21.5"
  },
  "pkg": {
    "assets": [
      "public/**/*",
      "plans/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ]
  }
}
