# Google Gemini API credentials
GEMINI_API_KEY="your_gemini_api_key_here"
GEMINI_MODEL="gemini-2.5-flash"
GEMINI_IMAGE_MODEL="imagen-3.0-generate"

# Toggle mock mode to avoid real API calls (recommended for tests)
MOCK_MODE=true

# Optional: Force GuardAgent to fail. Use 'true' for always fail, 'once' for a single failure.
FORCE_GUARD_FAIL=false

# Maximum number of RetryAgent attempts per Guard failure before stopping corrections
MAX_RETRY_ATTEMPTS=3
